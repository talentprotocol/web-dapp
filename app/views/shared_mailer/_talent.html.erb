<% profile_picture_url = attachments["profile_picture-#{talent.id}.png"]&.url || "https://talentprotocol-mvp.s3.eu-west-2.amazonaws.com/642d1cc7b48c7243d3df8e2f3c00ba1e.jpeg?X-Amz-Algorithm=AWS4-HMAC-SHA256&X-Amz-Credential=AKIAUTLBVWPO6V765TV7%2F20220719%2Feu-west-2%2Fs3%2Faws4_request&X-Amz-Date=20220719T082740Z&X-Amz-Expires=900&X-Amz-SignedHeaders=host&X-Amz-Signature=7715088059a72cfcb525f38f5595a2eea7cdff4a8fabfd625e433361f6ee96ed" %>
<% user = talent.user %>

<div class="row mb-5 border rounded-xl mx-0">
  <% if profile_picture_url %>
    <div class="col-1 py-3">
      <img class="img-fluid h-9 w-9 mx-auto rounded-image ay-middle ml-2" src=<%= profile_picture_url %> alt="User profile picture" />
    </div>
  <% end %>
  <div class="col-10 py-3">
    <p class="ay-middle user-title primary-2">
      &nbsp;&nbsp; <%= user.display_name || user.username %>
    </p>
    <p class="ay-middle primary-3 text-xs">
      &nbsp;&nbsp; <%= talent.occupation %>
    </p>
  </div>
</div>
